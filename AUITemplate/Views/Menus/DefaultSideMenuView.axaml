<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="140" d:DesignHeight="450"
             x:Class="AUITemplate.Views.Menus.DefaultSideMenuView"
			 xmlns:vm="using:AUITemplate.ViewModels.Menus"
             x:DataType="vm:DefaultSideMenuViewModel">
	<StackPanel HorizontalAlignment="Left">
		<Button Content=" Home" Command="{Binding HomeCommand}" Classes="SideMenuButton"/>
		<Button Content=" Apps" Command="{Binding AppsCommand}" Classes="SideMenuButton"/>
		<Button Content=" Settings" Command="{Binding SettingsCommand}" Classes="SideMenuButton"/>
		<Button Content=" User" Command="{Binding UserCommand}" Classes="SideMenuButton"/>
	</StackPanel>
</UserControl>
